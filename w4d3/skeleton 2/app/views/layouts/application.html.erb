<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <% if current_user %>
      <h1> <%= current_user.user_name %></h1>
      <%= button_to "Log Out", session_url, method: :delete %>
      <!-- <input type="submit" name="new_session_url" value="logout"> -->
    <% else  %>
      <h1>Nobody is logged in</h1>
      <%= button_to "Log In", new_session_url, method: :get %>
      <!-- <input type="submit" name="session_url" value="log in"> -->
    <% end %>
  </header>



<%= yield %>

</body>
</html>
